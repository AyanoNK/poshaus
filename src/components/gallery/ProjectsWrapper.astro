---
import GameGrid from "./GameGrid.astro";
import ProjectGrid from "./ProjectGrid.astro";

interface ICarrousel {
  image: string;
  slug: string;
}

interface Props {
  columns: ICarrousel[];
}

const { columns } = Astro.props;
---

<div class="flex flex-col items-center w-full m-0 p-4 gap-16">
  <ProjectGrid columns={columns} />
</div>
